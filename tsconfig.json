{
    "compilerOptions": {
        "target": "es2020",
        "module": "commonjs",
        "noEmit": true,
        "allowImportingTsExtensions": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "resolveJsonModule": true,
    }
}